<div class="forum-block title-fix">
  <h1>Q: {{question.content}} <button class="glyphicon glyphicon-star"{{action 'addToFavorites' question.content}}></button></h1>
  "{{question.note}}" <br>
  Username: {{question.author}}
</div>


{{#each question.answers as |answer|}}
<ul>
  <li class="display-none"></li>
  <li class="forum-block"><strong>A: {{answer.content}}</strong>
<br> Username: {{answer.author}}</li>
  <li class="display-none"></li>
</ul>
{{/each}}
